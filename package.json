    "axios": "^0.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "mocha": "^9.2.1",
    "mockery": "^2.1.0",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1"
  },
  "main": "lib/jserve.js",
  "bin": {
